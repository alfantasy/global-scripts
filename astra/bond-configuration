#!/bin/bash

OS_REJECT=""

if [ "$(id -u)" != "0" ]; then
    echo -e "\033[1;37m[i] –°–∫—Ä–∏–ø—Ç –¥–æ–ª–∂–µ–Ω –∏–º–µ—Ç—å –ø—Ä–∞–≤–∞ —Å—É–ø–µ—Ä–ø–æ–ª—å–∑–æ–≤–∞—Ç–µ–ª—è (root).\033[0m" 1>&2
    echo -e "\033[1;34m[i] –ó–∞–ø—É—Å—Ç–∏—Ç–µ —Å–∫—Ä–∏–ø—Ç —á–µ—Ä–µ–∑ sudo –∏–ª–∏ –∑–∞–π–¥–∏—Ç–µ –ø–æ–¥ —Å—É–ø–µ—Ä–ø–æ–ª—å–∑–æ–≤–∞—Ç–µ–ª—è –∏ –ø–æ–≤—Ç–æ—Ä–∏—Ç–µ –ø–æ–ø—ã—Ç–∫—É.\033[0m" 1>&2
    exit 1
fi

echo -e "\033[1;37m[i] –û–ø—Ä–µ–¥–µ–ª–µ–Ω–∏–µ —Å–∏—Å—Ç–µ–º—ã...\033[0m"
if [ -f /etc/os-release ]; then
    # Linux —Å modern init
    source /etc/os-release
    OS_REJECT="$PRETTY_NAME"
elif [ -f /etc/lsb-release ]; then
    # Ubuntu/Debian
    source /etc/lsb-release
    OS_REJECT="$DISTRIB_DESCRIPTION"    
else
    # Fallback
    OS_REJECT="$(uname -s)"
fi
sleep 2
clear

echo -e "\033[1;37m[i] –°–∫—Ä–∏–ø—Ç –ø–æ –Ω–∞—Å—Ç—Ä–æ–π–∫–µ –∏ —É—Å—Ç–∞–Ω–æ–≤–∫–∏ Bond-–∏–Ω—Ç–µ—Ä—Ñ–µ–π—Å–∞.\033[0m"

install_ifenslave_with_internet() {
    if apt install -y ifenslave >/dev/null 2>&1; then
        echo -e "\033[1;32m‚úÖ –ü–∞–∫–µ—Ç ifenslave —É—Å—Ç–∞–Ω–æ–≤–ª–µ–Ω\033[0m"
    else
        echo -e "\033[1;31m‚ùå –ü–∞–∫–µ—Ç ifenslave –Ω–µ —É—Å—Ç–∞–Ω–æ–≤–ª–µ–Ω\033[0m"
        echo -e "\033[1;31m‚ùå –ü—Ä–æ–≤–µ—Ä—å—Ç–µ –∏–Ω—Ç–µ—Ä–Ω–µ—Ç-—Å–æ–µ–¥–∏–Ω–µ–Ω–∏–µ –∏–ª–∏ /etc/apt/sources.list –∏ –ø–æ–≤—Ç–æ—Ä–∏—Ç–µ –ø–æ–ø—ã—Ç–∫—É\033[0m"
        exit 1
    fi
}

install_ifenslave_without_internet() {
    TMP_DEB=$(mktemp /tmp/ifenslave.XXXXXX.deb)
    tail -n +481 "$0" | base64 -d > "$TMP_DEB" # ENTER_LINK
    if dpkg -i "$TMP_DEB" >/dev/null 2>&1; then
        echo -e "\033[1;32m‚úÖ –ü–∞–∫–µ—Ç ifenslave —É—Å—Ç–∞–Ω–æ–≤–ª–µ–Ω\033[0m"
    else
        echo -e "\033[1;31m‚ùå –ü–∞–∫–µ—Ç ifenslave –Ω–µ —É—Å—Ç–∞–Ω–æ–≤–ª–µ–Ω\033[0m"
        exit 1
    fi
    rm -f "$TMP_DEB"
}

check_interfaces() {
    echo -e "\033[1;37m[i] –ü—Ä–æ–≤–µ—Ä–∫–∞ —Å–µ—Ç–µ–≤—ã—Ö –∏–Ω—Ç–µ—Ä—Ñ–µ–π—Å–æ–≤...\033[0m"
    
    # –ü–æ–ª—É—á–∞–µ–º —Å–ø–∏—Å–æ–∫ –∏–Ω—Ç–µ—Ä—Ñ–µ–π—Å–æ–≤ (–∏—Å–∫–ª—é—á–∞–µ–º lo, docker*, virbr*, wlan*, wifi*)
    local interfaces=($(ip link show | awk -F': ' '/^[0-9]+: [^l]/ && $2 !~ /^lo/ && $2 !~ /^docker/ && $2 !~ /^virbr/ && $2 !~ /^wlan/ && $2 !~ /^wifi/ && $2 !~ /^veth/ {print $2}' | sed 's/@.*//'))
    
    if [ ${#interfaces[@]} -eq 0 ]; then
        echo -e "\033[1;31m‚ùå –û—à–∏–±–∫–∞: –ù–µ –Ω–∞–π–¥–µ–Ω–æ –Ω–∏ –æ–¥–Ω–æ–≥–æ –∞–∫—Ç–∏–≤–Ω–æ–≥–æ –∏–Ω—Ç–µ—Ä—Ñ–µ–π—Å–∞ (–∫—Ä–æ–º–µ Wi-Fi –∏ loopback)\033[0m"
        echo -e "\033[1;37m[i] –ü–æ–∂–∞–ª—É–π—Å—Ç–∞, –ø–æ–¥–∫–ª—é—á–∏—Ç–µ —Å–µ—Ç–µ–≤—ã–µ –∫–∞–±–µ–ª–∏ –∏ –ø—Ä–æ–≤–µ—Ä—å—Ç–µ –∏–Ω—Ç–µ—Ä—Ñ–µ–π—Å—ã.\033[0m"
        exit 1
    elif [ ${#interfaces[@]} -eq 1 ]; then
        echo -e "\033[1;33m‚ö†Ô∏è  –ù–∞–π–¥–µ–Ω —Ç–æ–ª—å–∫–æ –æ–¥–∏–Ω –∏–Ω—Ç–µ—Ä—Ñ–µ–π—Å: ${interfaces[0]}\033[0m"
        echo -e "\033[1;37m[i] –î–ª—è –æ–±—ä–µ–¥–∏–Ω–µ–Ω–∏—è –∫–∞–Ω–∞–ª–æ–≤ —Ç—Ä–µ–±—É–µ—Ç—Å—è –º–∏–Ω–∏–º—É–º 2 –∏–Ω—Ç–µ—Ä—Ñ–µ–π—Å–∞.\033[0m"
        exit 1
    fi
    
    # –ü–æ–∫–∞–∑—ã–≤–∞–µ–º –Ω–∞–π–¥–µ–Ω–Ω—ã–µ –∏–Ω—Ç–µ—Ä—Ñ–µ–π—Å—ã
    echo -e "\033[1;32m‚úÖ –ù–∞–π–¥–µ–Ω–æ –∏–Ω—Ç–µ—Ä—Ñ–µ–π—Å–æ–≤: ${#interfaces[@]}\033[0m"
    echo -e "\033[1;37m–°–ø–∏—Å–æ–∫ –¥–æ—Å—Ç—É–ø–Ω—ã—Ö –∏–Ω—Ç–µ—Ä—Ñ–µ–π—Å–æ–≤:\033[0m"
    for i in "${!interfaces[@]}"; do
        echo -e "  \033[1;36m$((i+1)). ${interfaces[$i]}\033[0m"
        
        # –ü–æ–∫–∞–∑—ã–≤–∞–µ–º IP –∞–¥—Ä–µ—Å –µ—Å–ª–∏ –µ—Å—Ç—å
        local ip_addr=$(ip -4 addr show "${interfaces[$i]}" 2>/dev/null | grep inet | awk '{print $2}' | cut -d/ -f1)
        if [ -n "$ip_addr" ]; then
            echo -e "     üì° IP: $ip_addr"
        fi
        
        # –ü–æ–∫–∞–∑—ã–≤–∞–µ–º —Å–∫–æ—Ä–æ—Å—Ç—å –µ—Å–ª–∏ –¥–æ—Å—Ç—É–ø–Ω–æ
        local speed=$(cat /sys/class/net/"${interfaces[$i]}"/speed 2>/dev/null || echo "–Ω–µ–∏–∑–≤–µ—Å—Ç–Ω–æ")
        echo -e "     ‚ö° –°–∫–æ—Ä–æ—Å—Ç—å: ${speed} –ú–±–∏—Ç/—Å"
    done
    
    # –ê–≤—Ç–æ–º–∞—Ç–∏—á–µ—Å–∫–∏–π –≤—ã–±–æ—Ä –ø–µ—Ä–≤—ã—Ö –¥–≤—É—Ö –∏–Ω—Ç–µ—Ä—Ñ–µ–π—Å–æ–≤
    local iface1="${interfaces[0]}"
    local iface2="${interfaces[1]}"
    
    echo -e "\n\033[1;37m[i] –ê–≤—Ç–æ–º–∞—Ç–∏—á–µ—Å–∫–∏ –≤—ã–±—Ä–∞–Ω—ã –∏–Ω—Ç–µ—Ä—Ñ–µ–π—Å—ã:\033[0m"
    echo -e "   \033[1;32m–û—Å–Ω–æ–≤–Ω–æ–π: $iface1\033[0m"
    echo -e "   \033[1;32m–í—Ç–æ—Ä–æ–π: $iface2\033[0m"
    
    # –ó–∞–ø—Ä–æ—Å –ø–æ–¥—Ç–≤–µ—Ä–∂–¥–µ–Ω–∏—è
    local timeout=10
    local reply=""
    
    echo -e "\n\033[1;33m[?] –ò—Å–ø–æ–ª—å–∑–æ–≤–∞—Ç—å –≤—ã–±—Ä–∞–Ω–Ω—ã–µ –∏–Ω—Ç–µ—Ä—Ñ–µ–π—Å—ã –¥–ª—è –æ–±—ä–µ–¥–∏–Ω–µ–Ω–∏—è?\033[0m"
    echo -e "    –ï—Å–ª–∏ –Ω—É–∂–Ω–æ –∏–∑–º–µ–Ω–∏—Ç—å –≤—ã–±–æ—Ä, –≤–≤–µ–¥–∏—Ç–µ 'n' –∏ –Ω–∞–∂–º–∏—Ç–µ Enter"
    echo -ne "    \033[1;37m–ü—Ä–æ–¥–æ–ª–∂–∏—Ç—å? (Y/n) [–ê–≤—Ç–æ —á–µ—Ä–µ–∑ ${timeout} —Å–µ–∫]: \033[0m"
    
    # –ß–∏—Ç–∞–µ–º –æ—Ç–≤–µ—Ç
    if read -t $timeout -n 1 reply; then
        echo  # –ù–æ–≤–∞—è —Å—Ç—Ä–æ–∫–∞ –ø–æ—Å–ª–µ –≤–≤–æ–¥–∞
    else
        echo -e "\n\033[1;37m[i] –¢–∞–π–º–∞—É—Ç. –ò—Å–ø–æ–ª—å–∑—É–µ—Ç—Å—è –≤—ã–±–æ—Ä –ø–æ —É–º–æ–ª—á–∞–Ω–∏—é (Y).\033[0m"
        reply="y"
    fi
    
    # –û–±—Ä–∞–±–æ—Ç–∫–∞ –æ—Ç–≤–µ—Ç–∞
    if [[ "$reply" =~ ^[Nn]$ ]]; then
        echo -e "\033[1;37m[i] –í—ã–±–µ—Ä–∏—Ç–µ –∏–Ω—Ç–µ—Ä—Ñ–µ–π—Å—ã –≤—Ä—É—á–Ω—É—é:\033[0m"
        
        # –ó–∞–ø—Ä–æ—Å –ø–µ—Ä–≤–æ–≥–æ –∏–Ω—Ç–µ—Ä—Ñ–µ–π—Å–∞
        while true; do
            echo -ne "–í–≤–µ–¥–∏—Ç–µ –Ω–æ–º–µ—Ä –ø–µ—Ä–≤–æ–≥–æ –∏–Ω—Ç–µ—Ä—Ñ–µ–π—Å–∞ (1-${#interfaces[@]}): "
            read -r choice1
            if [[ "$choice1" =~ ^[0-9]+$ ]] && [ "$choice1" -ge 1 ] && [ "$choice1" -le ${#interfaces[@]} ]; then
                iface1="${interfaces[$((choice1-1))]}"
                break
            else
                echo -e "\033[1;31m–ù–µ–≤–µ—Ä–Ω—ã–π –≤—ã–±–æ—Ä. –ü–æ–ø—Ä–æ–±—É–π—Ç–µ —Å–Ω–æ–≤–∞.\033[0m"
            fi
        done
        
        # –ó–∞–ø—Ä–æ—Å –≤—Ç–æ—Ä–æ–≥–æ –∏–Ω—Ç–µ—Ä—Ñ–µ–π—Å–∞
        while true; do
            echo -ne "–í–≤–µ–¥–∏—Ç–µ –Ω–æ–º–µ—Ä –≤—Ç–æ—Ä–æ–≥–æ –∏–Ω—Ç–µ—Ä—Ñ–µ–π—Å–∞ (1-${#interfaces[@]}), –∫—Ä–æ–º–µ $choice1: "
            read -r choice2
            if [[ "$choice2" =~ ^[0-9]+$ ]] && [ "$choice2" -ge 1 ] && [ "$choice2" -le ${#interfaces[@]} ] && [ "$choice2" -ne "$choice1" ]; then
                iface2="${interfaces[$((choice2-1))]}"
                break
            else
                echo -e "\033[1;31m–ù–µ–≤–µ—Ä–Ω—ã–π –≤—ã–±–æ—Ä. –ü–æ–ø—Ä–æ–±—É–π—Ç–µ —Å–Ω–æ–≤–∞.\033[0m"
            fi
        done
        
        echo -e "\033[1;32m‚úÖ –í—ã–±—Ä–∞–Ω—ã –∏–Ω—Ç–µ—Ä—Ñ–µ–π—Å—ã: $iface1 –∏ $iface2\033[0m"
    fi
    
    # –í–æ–∑–≤—Ä–∞—â–∞–µ–º –≤—ã–±—Ä–∞–Ω–Ω—ã–µ –∏–Ω—Ç–µ—Ä—Ñ–µ–π—Å—ã
    SELECTED_IFACES=("$iface1" "$iface2")
    
    # –î–æ–ø–æ–ª–Ω–∏—Ç–µ–ª—å–Ω–∞—è –ø—Ä–æ–≤–µ—Ä–∫–∞
    echo -e "\033[1;37m[i] –ü—Ä–æ–≤–µ—Ä–∫–∞ –≤—ã–±—Ä–∞–Ω–Ω—ã—Ö –∏–Ω—Ç–µ—Ä—Ñ–µ–π—Å–æ–≤...\033[0m"
    for iface in "${SELECTED_IFACES[@]}"; do
        if ! ip link show "$iface" >/dev/null 2>&1; then
            echo -e "\033[1;31m‚ùå –û—à–∏–±–∫–∞: –ò–Ω—Ç–µ—Ä—Ñ–µ–π—Å $iface –Ω–µ —Å—É—â–µ—Å—Ç–≤—É–µ—Ç!\033[0m"
            exit 1
        fi
        
        # –ü—Ä–æ–≤–µ—Ä—è–µ–º, —á—Ç–æ –∏–Ω—Ç–µ—Ä—Ñ–µ–π—Å –Ω–µ –æ—Ç–∫–ª—é—á–µ–Ω
        if ip link show "$iface" | grep -q "state DOWN"; then
            echo -e "\033[1;33m‚ö†Ô∏è  –í–Ω–∏–º–∞–Ω–∏–µ: –ò–Ω—Ç–µ—Ä—Ñ–µ–π—Å $iface –≤—ã–∫–ª—é—á–µ–Ω. –í–∫–ª—é—á–∏—Ç—å? (Y/n)\033[0m"
            read -t 5 -n 1 enable_reply || enable_reply="y"
            if [[ "$enable_reply" =~ ^[Yy]$ ]]; then
                sudo ip link set "$iface" up
                echo -e "\033[1;32m‚úÖ –ò–Ω—Ç–µ—Ä—Ñ–µ–π—Å $iface –≤–∫–ª—é—á–µ–Ω\033[0m"
            fi
        fi
    done
    
    echo -e "\033[1;32m‚úÖ –í—Å–µ –ø—Ä–æ–≤–µ—Ä–∫–∏ –ø—Ä–æ–π–¥–µ–Ω—ã. –ù–∞—á–∏–Ω–∞–µ–º –Ω–∞—Å—Ç—Ä–æ–π–∫—É...\033[0m"
    return 0
}

cidr_to_netmask() {
    local cidr=$1
    local netmask=""
    local full_octets=$((cidr / 8))
    local partial_octet=$((cidr % 8))
    
    # –ó–∞–ø–æ–ª–Ω—è–µ–º –ø–æ–ª–Ω—ã–µ –æ–∫—Ç–µ—Ç—ã
    for ((i=0; i<full_octets; i++)); do
        netmask+="255."
    done
    
    # –û–±—Ä–∞–±–∞—Ç—ã–≤–∞–µ–º —á–∞—Å—Ç–∏—á–Ω—ã–π –æ–∫—Ç–µ—Ç
    if [ $partial_octet -gt 0 ]; then
        local mask=$((256 - 2**(8 - partial_octet)))
        netmask+="$mask"
        ((full_octets++))
    else
        # –£–±–∏—Ä–∞–µ–º –ª–∏—à–Ω—é—é —Ç–æ—á–∫—É
        netmask="${netmask%.}"
    fi
    
    # –ó–∞–ø–æ–ª–Ω—è–µ–º –æ—Å—Ç–∞–≤—à–∏–µ—Å—è –Ω—É–ª—è–º–∏
    for ((i=full_octets; i<4; i++)); do
        if [ $i -eq 3 ]; then
            netmask+=".0"
        else
            netmask+=".0"
        fi
    done
    
    # –£–±–∏—Ä–∞–µ–º –≤–æ–∑–º–æ–∂–Ω—ã–µ –ª–∏—à–Ω–∏–µ —Ç–æ—á–∫–∏
    netmask=$(echo "$netmask" | sed 's/\.\././g' | sed 's/^\.//' | sed 's/\.$//')
    
    # –ü—Ä–æ–≤–µ—Ä—è–µ–º —á—Ç–æ –ø–æ–ª—É—á–∏–ª–æ—Å—å 4 –æ–∫—Ç–µ—Ç–∞
    local octets=$(echo "$netmask" | tr '.' ' ' | wc -w)
    if [ $octets -ne 4 ]; then
        echo "–û—à–∏–±–∫–∞: –Ω–µ–∫–æ—Ä—Ä–µ–∫—Ç–Ω–∞—è –º–∞—Å–∫–∞" >&2
        return 1
    fi
    
    echo "$netmask"
    return 0
}

first_settings() {
    local timeout=5
    local reply=""
    local default_choice="y"

    echo -e "\033[1;37m[i] –§–ê–ô–õ –î–õ–Ø –õ–û–ì–ò–†–û–í–ê–ù–ò–Ø –í–ê–ñ–ù–´–• –°–û–û–ë–©–ï–ù–ò–ô: /var/log/bond-configuration.log\033[0m"
    echo -e "\033[1;37m[i] –ü—Ä–æ—á–∏—Ç–∞—Ç—å –µ–≥–æ –º–æ–∂–Ω–æ –ø—Ä–∏ –ø–æ–º–æ—â–∏ –∫–æ–º–∞–Ω–¥—ã: cat /var/log/bond-configuration.log\033[0m"]

    echo -e "\033[1;37m[i] –ü–æ—Å–ª–µ –¥–∞–Ω–Ω—ã—Ö –Ω–∞—Å—Ç—Ä–æ–µ–∫, –∏–Ω—Ç–µ—Ä–Ω–µ—Ç –º–æ–∂–µ—Ç –Ω–µ –ø–æ—è–≤–∏—Ç—Å—è –∞–≤—Ç–æ–º–∞—Ç–∏—á–µ—Å–∫–∏, –ø–æ—ç—Ç–æ–º—É –í–∞–º –Ω—É–∂–Ω–æ –±—É–¥–µ—Ç —É–±–µ–¥–∏—Ç—Å—è, —á—Ç–æ –æ–±–∞ –∏–Ω—Ç–µ—Ä—Ñ–µ–π—Å–∞ —Ä–∞–±–æ—Ç–∞—é—Ç –∏ –∏—Ö —Ç–æ—á–Ω–æ –±–æ–ª–µ–µ –æ–¥–Ω–æ–≥–æ.\033[0m"
    
    read -t $timeout -p $'\033[1;33m[?]\033[0m –í—ã —É–≤–µ—Ä–µ–Ω—ã, —á—Ç–æ —Ö–æ—Ç–∏—Ç–µ –ø—Ä–æ–¥–æ–ª–∂–∏—Ç—å? (y/n) [–ê–≤—Ç–æ —á–µ—Ä–µ–∑ \033[1;37m'"${timeout}"$'\033[0m —Å–µ–∫]: ' -n 1 reply
    
    local read_status=$?
    
    echo
    
    if [[ $read_status -gt 128 ]]; then
        echo -e "\033[1;37m[i] –¢–∞–π–º–∞—É—Ç. –ò—Å–ø–æ–ª—å–∑—É–µ—Ç—Å—è –≤—ã–±–æ—Ä –ø–æ —É–º–æ–ª—á–∞–Ω–∏—é (Y).\033[0m"
        reply="$default_choice"
    fi

    if [[ -z "$reply" ]] || [[ "$reply" =~ ^[Yy]$ ]]; then
        echo -e "\033[1;37m[i] –†–∞–±–æ—Ç–∞ —Å–∫—Ä–∏–ø—Ç–∞ –ø—Ä–æ–¥–æ–ª–∂–∞–µ—Ç—Å—è. –ü–µ—Ä–≤—ã–π —ç—Ç–∞–ø - —É—Å—Ç–∞–Ω–æ–≤–∫–∞ –Ω–µ–æ–±—Ö–æ–¥–∏–º—ã—Ö –ø–∞–∫–µ—Ç–æ–≤.\033[0m"
    else
        echo -e "\033[1;37m[i] –†–∞–±–æ—Ç–∞ —Å–∫—Ä–∏–ø—Ç–∞ –æ—Å—Ç–∞–Ω–æ–≤–ª–µ–Ω–∞.\033[0m"
        exit 1
    fi

    sleep 1
    
    if dpkg -l ifenslave >/dev/null 2>&1; then
        echo -e "\033[1;32m‚úÖ –ü–∞–∫–µ—Ç ifenslave —É—Å—Ç–∞–Ω–æ–≤–ª–µ–Ω\033[0m"
    else
        if ping -c 1 google.com >/dev/null 2>&1; then
            echo -e "\033[1;32m‚úÖ –ò–Ω—Ç–µ—Ä–Ω–µ—Ç –¥–æ—Å—Ç—É–ø–µ–Ω.\033[0m"
            install_ifenslave_with_internet
        else
            echo -e "\033[1;31m‚ùå –ò–Ω—Ç–µ—Ä–Ω–µ—Ç –Ω–µ–¥–æ—Å—Ç—É–ø–µ–Ω. –ü—Ä–æ–≤–µ—Ä—å—Ç–µ –ø–æ–¥–∫–ª—é—á–µ–Ω–∏–µ.\033[0m"
            install_ifenslave_without_internet
        fi
    fi

    check_interfaces
    echo -e "\033[1;37m[i] –í—ã–≤–æ–¥ –ø–æ—Å–ª–µ –ø—Ä–æ–≤–µ—Ä–∫–∏: ${SELECTED_IFACES[@]}\033[0m"

    echo -e "\033[1;37m[i] –ó–∞–ø–æ–ª–Ω—è–µ—Ç—Å—è –æ—Å–Ω–æ–≤–Ω–æ–π —Ñ–∞–π–ª –∫–æ–Ω—Ñ–∏–≥—É—Ä–∞—Ü–∏–∏ —Å–µ—Ç–∏.\033[0m"
    echo -e "\033[1;37m[i] –í–Ω–∏–º–∞–Ω–∏–µ! –ò–Ω—Ç–µ—Ä—Ñ–µ–π—Å—ã –≤—ã–±—Ä–∞–Ω–Ω—ã–µ –≤—ã—à–µ –±—É–¥—É—Ç –æ—Ç–∫–ª—é—á–µ–Ω—ã –æ—Ç —É–ø—Ä–∞–≤–ª–µ–Ω–∏—è —Å–µ—Ä–≤–∏—Å–æ–º NetworkManager.\033[0m"
    sleep 2

    for iface in "${SELECTED_IFACES[@]}"; do
        if ! ip link show "$iface" >/dev/null 2>&1; then
            echo -e "\033[1;31m‚ùå –û—à–∏–±–∫–∞: –ò–Ω—Ç–µ—Ä—Ñ–µ–π—Å $iface –Ω–µ —Å—É—â–µ—Å—Ç–≤—É–µ—Ç!\033[0m"
            exit 1
        fi
        nmcli device set "$iface" managed off
    done

    NON_CONF_FILE=$(cat /etc/network/interfaces)

    if [ -z "$NON_CONF_FILE" ]; then
        echo -e "\033[1;31m –§–∞–π–ª –∫–æ–Ω—Ñ–∏–≥—É—Ä–∞—Ü–∏–∏ /etc/network/interfaces –ø—É—Å—Ç!\033[0m"
        cat <<EOF > /etc/network/interfaces
# This file describes the network interfaces available on your system
# and how to activate them. For more information, see interfaces(5).

source /etc/network/interfaces.d/*
EOF
    fi

    local config_type=""
    local ip_address=""
    local netmask=""
    local gateway=""
    local bond_ip_address=""
    
    echo -e "\033[1;37m[i] –ù–∞—Å—Ç—Ä–æ–π–∫–∞ –æ–±—â–µ–≥–æ –∏–Ω—Ç–µ—Ä—Ñ–µ–π—Å–∞ (bond)\033[0m"
    
    # –í—ã–±–æ—Ä —Ç–∏–ø–∞ –Ω–∞—Å—Ç—Ä–æ–π–∫–∏
    echo -ne "\033[1;33m[?] –ù–∞—Å—Ç—Ä–æ–π–∫–∞ –∏–Ω—Ç–µ—Ä—Ñ–µ–π—Å–∞ (dhcp/static) [dhcp]: \033[0m"
    if read -t 10 -r config_type && [ -n "$config_type" ]; then
        config_type=$(echo "$config_type" | tr '[:upper:]' '[:lower:]')
    else
        config_type="dhcp"
        echo -e "\n\033[1;37m[i] –ò—Å–ø–æ–ª—å–∑—É–µ—Ç—Å—è –∑–Ω–∞—á–µ–Ω–∏–µ –ø–æ —É–º–æ–ª—á–∞–Ω–∏—é: $config_type\033[0m"
    fi
    
    case $config_type in
        dhcp|"")
            echo -e "\033[1;32m‚úÖ –í—ã–±—Ä–∞–Ω DHCP\033[0m"
            ;;
        static)
            # –ó–∞–ø—Ä–æ—Å IP —Å –º–∞—Å–∫–æ–π –≤ —Ñ–æ—Ä–º–∞—Ç–µ CIDR
            while true; do
                echo -ne "\033[1;33m[?] –í–≤–µ–¥–∏—Ç–µ IP-–∞–¥—Ä–µ—Å —Å –º–∞—Å–∫–æ–π (–Ω–∞–ø—Ä–∏–º–µ—Ä, 192.168.1.100/24): \033[0m"
                read -r ip_cidr
                
                if [[ -z "$ip_cidr" ]]; then
                    echo -e "\033[1;31m‚ùå IP-–∞–¥—Ä–µ—Å –Ω–µ –º–æ–∂–µ—Ç –±—ã—Ç—å –ø—É—Å—Ç—ã–º\033[0m"
                    continue
                fi
                
                # –ü—Ä–æ–≤–µ—Ä–∫–∞ —Ñ–æ—Ä–º–∞—Ç–∞ CIDR
                if [[ ! "$ip_cidr" =~ ^[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}/[0-9]{1,2}$ ]]; then
                    echo -e "\033[1;31m‚ùå –ù–µ–≤–µ—Ä–Ω—ã–π —Ñ–æ—Ä–º–∞—Ç. –ü—Ä–∏–º–µ—Ä: 192.168.1.100/24\033[0m"
                    continue
                fi
                
                # –†–∞–∑–¥–µ–ª—è–µ–º IP –∏ –ø—Ä–µ—Ñ–∏–∫—Å
                IFS='/' read -r ip_addr cidr <<< "$ip_cidr"
                
                # –ü—Ä–æ–≤–µ—Ä–∫–∞ –≤–∞–ª–∏–¥–Ω–æ—Å—Ç–∏ IP
                IFS='.' read -r ip1 ip2 ip3 ip4 <<< "$ip_addr"
                if [[ $ip1 -gt 255 || $ip2 -gt 255 || $ip3 -gt 255 || $ip4 -gt 255 ]] || 
                   [[ $cidr -lt 1 || $cidr -gt 32 ]]; then
                    echo -e "\033[1;31m‚ùå –ù–µ–∫–æ—Ä—Ä–µ–∫—Ç–Ω—ã–π IP –∏–ª–∏ –º–∞—Å–∫–∞\033[0m"
                    continue
                fi
                
                # –ü—Ä–µ–æ–±—Ä–∞–∑–æ–≤–∞–Ω–∏–µ CIDR –≤ –æ–±—ã—á–Ω—É—é –º–∞—Å–∫—É
                netmask=$(cidr_to_netmask "$cidr")
                if [[ $? -ne 0 ]]; then
                    echo -e "\033[1;31m‚ùå –û—à–∏–±–∫–∞ –ø—Ä–µ–æ–±—Ä–∞–∑–æ–≤–∞–Ω–∏—è –º–∞—Å–∫–∏\033[0m"
                    continue
                fi
                
                break
            done
            
            # –ó–∞–ø—Ä–æ—Å —à–ª—é–∑–∞
            while true; do
                echo -ne "\033[1;33m[?] –í–≤–µ–¥–∏—Ç–µ —à–ª—é–∑ (–Ω–∞–ø—Ä–∏–º–µ—Ä, 192.168.1.1): \033[0m"
                read -r gateway
                
                if [[ -z "$gateway" ]]; then
                    echo -e "\033[1;33m‚ö†Ô∏è  –®–ª—é–∑ –Ω–µ —É–∫–∞–∑–∞–Ω. –ü—Ä–æ–¥–æ–ª–∂–∏—Ç—å? (Y/n): \033[0m"
                    read -t 5 -n 1 reply || reply="y"
                    [[ "$reply" =~ ^[Yy]$ ]] && break
                    continue
                fi
                
                # –ü—Ä–æ–≤–µ—Ä–∫–∞ —Ñ–æ—Ä–º–∞—Ç–∞ —à–ª—é–∑–∞
                if [[ ! "$gateway" =~ ^[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}$ ]]; then
                    echo -e "\033[1;31m‚ùå –ù–µ–≤–µ—Ä–Ω—ã–π —Ñ–æ—Ä–º–∞—Ç —à–ª—é–∑–∞\033[0m"
                    continue
                fi
                
                IFS='.' read -r gw1 gw2 gw3 gw4 <<< "$gateway"
                if [[ $gw1 -gt 255 || $gw2 -gt 255 || $gw3 -gt 255 || $gw4 -gt 255 ]]; then
                    echo -e "\033[1;31m‚ùå –ù–µ–∫–æ—Ä—Ä–µ–∫—Ç–Ω—ã–π IP —à–ª—é–∑–∞\033[0m"
                    continue
                fi
                
                break
            done
            
            echo -e "\033[1;32m‚úÖ –°—Ç–∞—Ç–∏—á–µ—Å–∫–∏–π IP: $ip_addr\033[0m"
            echo -e "\033[1;32m‚úÖ –ú–∞—Å–∫–∞ –ø–æ–¥—Å–µ—Ç–∏: $netmask\033[0m"
            echo -e "\033[1;32m‚úÖ –®–ª—é–∑: ${gateway:-–Ω–µ —É–∫–∞–∑–∞–Ω}\033[0m"
            ;;
        *)
            echo -e "\033[1;31m‚ùå –û—à–∏–±–∫–∞: –¥–æ–ø—É—Å—Ç–∏–º—ã–µ –∑–Ω–∞—á–µ–Ω–∏—è: dhcp –∏–ª–∏ static\033[0m"
            exit 1
            ;;
    esac
    
    echo -e "\033[1;37m[?] –°–µ–π—á–∞—Å –í–∞–º –Ω–µ–æ–±—Ö–æ–¥–∏–º–æ –≤—ã–±—Ä–∞—Ç—å IP-–∞–¥—Ä–µ—Å, –∫–æ—Ç–æ—Ä—ã–π –±—É–¥–µ—Ç –∏—Å–ø–æ–ª—å–∑–æ–≤–∞—Ç—å—Å—è –¥–ª—è –±–æ–Ω–¥–∏—Ä–æ–≤–∞–Ω–∏—è.\033[0m"
    echo -e "\033[1;37m[i] –ë–æ–Ω–¥–∏—Ä–æ–≤–∞–Ω–∏–µ - —ç—Ç–æ —á–∞—Å—Ç—å —Ä–µ–∑–µ—Ä–≤–∏—Ä–æ–≤–∞–Ω–∏–µ –∏ –æ—Å—É—â–µ—Å—Ç–≤–ª—è–µ—Ç—Å—è –ø—Ä–∏ –ø–æ–º–æ—â–∏ –æ—Ç–ø—Ä–∞–≤–∫–∏ ping-arp –¥–æ —É–∫–∞–∑–∞–Ω–Ω–æ–≥–æ —É–∑–ª–∞.\033[0m"
    read -p "–í–≤–µ–¥–∏—Ç–µ IP-–∞–¥—Ä–µ—Å, –∫–æ—Ç–æ—Ä—ã–π –±—É–¥–µ—Ç –∏—Å–ø–æ–ª—å–∑–æ–≤–∞—Ç—å—Å—è –¥–ª—è –±–æ–Ω–¥–∏—Ä–æ–≤–∞–Ω–∏—è (—Ñ–æ—Ä–º–∞—Ç: 192.168.1.100): " -r bond_ip_address
    
    # –ü—Ä–æ–≤–µ—Ä–∫–∞ –ø—Ä–∞–≤–∏–ª—å–Ω–æ—Å—Ç–∏ —Ñ–æ—Ä–º–∞—Ç–∞ IP
    if [[ ! "$bond_ip_address" =~ ^[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}$ ]]; then
        echo -e "\033[1;31m‚ùå –û—à–∏–±–∫–∞: –Ω–µ–≤–µ—Ä–Ω—ã–π —Ñ–æ—Ä–º–∞—Ç IP-–∞–¥—Ä–µ—Å–∞. –ò—Å–ø–æ–ª—å–∑—É–π—Ç–µ —Ñ–æ—Ä–º–∞—Ç: 192.168.1.100\033[0m"
        exit 1
    fi

    IFS='.' read -r ip1 ip2 ip3 ip4 <<< "$bond_ip_address"
    
    if [[ $ip1 -gt 255 || $ip2 -gt 255 || $ip3 -gt 255 || $ip4 -gt 255 ]]; then
        echo -e "\033[1;31m‚ùå –û—à–∏–±–∫–∞: –Ω–µ–∫–æ—Ä—Ä–µ–∫—Ç–Ω—ã–π IP-–∞–¥—Ä–µ—Å\033[0m"
        exit 1
    fi    

    # –ü–æ–¥—Ç–≤–µ—Ä–∂–¥–µ–Ω–∏–µ
    echo -e "\n\033[1;37m[i] –ò—Ç–æ–≥–æ–≤—ã–µ –Ω–∞—Å—Ç—Ä–æ–π–∫–∏:\033[0m"
    echo -e "   –¢–∏–ø: $config_type"
    if [[ "$config_type" == "static" ]]; then
        echo -e "   IP: $ip_addr"
        echo -e "   –ú–∞—Å–∫–∞: $netmask"
        echo -e "   –®–ª—é–∑: ${gateway:-–Ω–µ —É–∫–∞–∑–∞–Ω}"
    fi

    local STR="
# This file generated and configurated by bond-configuration.
# File describes the network interfaces available on your system
# and how to activate them. For more information, see interfaces(5).

# TIP: If your interfaces don't work, remove this file
# and select all interfaces managed by NetworkManager.    

auto lo ${SELECTED_IFACES[0]} ${SELECTED_IFACES[1]} bond0
iface lo inet loopback

# First slave interface
iface ${SELECTED_IFACES[0]} inet manual
    post-up ip link set ${SELECTED_IFACES[0]} promisc on
    post-down ip link set ${SELECTED_IFACES[0]} promisc off

# Second slave interface
iface ${SELECTED_IFACES[1]} inet manual
    post-up ip link set ${SELECTED_IFACES[1]} promisc on
    post-down ip link set ${SELECTED_IFACES[1]} promisc off

# Bond interface
iface bond0 inet ${config_type}"

    if [[ "$config_type" == "static" ]]; then    
        STR+="
    address ${ip_addr}
    netmask ${netmask}"

        if [[ -n "$gateway" ]]; then
            STR+="
    gateway ${gateway}"
        fi
    fi

    STR+="
    post-up ip link set bond0 promisc on
    post-down ip link set bond0 promisc off
    bond-mode active-backup
    bond-miimon 100
    bond-downdelay 200
    bond-updelay 150
    bond-slaves ${SELECTED_IFACES[0]} ${SELECTED_IFACES[1]}"

    if [[ -n "$bond_ip_address" ]]; then
        STR+="
    bond-arp-interval 100
    bond-arp-ip-target ${bond_ip_address}"
    fi

    echo "$STR" > /etc/network/interfaces.d/bond-configuration

    echo "System must be restarted." > /var/log/bond-configuration.log

    echo -e "\033[1;37m[i] –ù–∞—Å—Ç—Ä–æ–π–∫–∞ –∏ —É—Å—Ç–∞–Ω–æ–≤–∫–∞ Bond-–∏–Ω—Ç–µ—Ä—Ñ–µ–π—Å–∞ –∑–∞–≤–µ—Ä—à–µ–Ω–∞.\033[0m"

    echo -e "\033[1;37m[i] –ü–µ—Ä–µ–∑–∞–≥—Ä—É–∑–∫–∞ —Å–∏—Å—Ç–µ–º—ã...\033[0m"
    sleep 2
    reboot
}

if echo "$OS_REJECT" | grep -q "Astra"; then
    echo -e "\033[1;37m[i] –°–∏—Å—Ç–µ–º–∞ –æ–ø—Ä–µ–¥–µ–ª–µ–Ω–∞, —É –í–∞—Å —Å—Ç–æ–∏—Ç Astra Linux.\033[0m"
    echo -e "\033[1;37m[i] –ù–∞—Å—Ç—Ä–æ–π–∫–∞ –∏ —É—Å—Ç–∞–Ω–æ–≤–∫–∞ Bond-–∏–Ω—Ç–µ—Ä—Ñ–µ–π—Å–∞ –Ω–∞—á–Ω–µ—Ç—Å—è –∞–≤—Ç–æ–º–∞—Ç–∏—á–µ—Å–∫–∏.\033[0m"
    sleep 2
    first_settings
else
    echo -e "\033[1;31m‚ùå –°–∏—Å—Ç–µ–º–∞ –Ω–µ —Å–æ–≤–ø–∞–¥–∞–µ—Ç —Å Astra Linux.\033[0m"
    echo -e "\033[1;34m[i] –û–¥–Ω–∞–∫–æ, –µ—Å–ª–∏ –ø—Ä–æ–∏–∑–æ—à–ª–∞ –æ—à–∏–±–∫–∞, –í—ã –≤—Å–µ —Ä–∞–≤–Ω–æ –º–æ–∂–µ—Ç–µ –≤—ã–ø–æ–ª–Ω–∏—Ç—å –¥–∞–Ω–Ω—ã–π —Å–∫—Ä–∏–ø—Ç.\033[0m"
    read -p "[?] –í—ã —É–≤–µ—Ä–µ–Ω—ã, —á—Ç–æ —Ö–æ—Ç–∏—Ç–µ –ø—Ä–æ–¥–æ–ª–∂–∏—Ç—å? (y/n): " -n 1 -r
    if [[ $REPLY =~ ^[Yy]$ ]]; then
        echo -e "\033[1;37m[i] –ü–æ–¥–æ–∂–¥–∏—Ç–µ, –ø–æ–∂–∞–ª—É–π—Å—Ç–∞...\033[0m"
        sleep 2
        first_settings
    else
        exit 0
    fi
fi
exit 0
